# read data
 read_data(file=chi.chi3,type=chi,group=data)
 my.k = data.k
 my.chi = data.chi/(data.k**3)
 # Interpolate to grid
 newdata.k = range(0,10.0,0.05)
 newdata.chi = qinterp(my.k, my.chi, newdata.k)
 # Guess e0 value
 guess e = 0.0
 set s = 1.0
 guess s1 = 0.0025
 guess s2 = 0.0005
 guess s3 = 0.0005
 guess s4 = 0.0005
 guess dr = 0.02
 # Read Paths
path(1, file=feff0001.dat, e0 = e, s02 = s, sigma2 = s1)
path(2, file=feff0002.dat, e0 = e, s02 = s, sigma2 = s1)
path(3, file=feff0003.dat, e0 = e, s02 = s, sigma2 = s1)
path(4, file=feff0004.dat, e0 = e, s02 = s, sigma2 = s1)
path(5, file=feff0005.dat, e0 = e, s02 = s, sigma2 = s1)
path(6, file=feff0006.dat, e0 = e, s02 = s, sigma2 = s1)
path(7, file=feff0007.dat, e0 = e, s02 = s, sigma2 = s1)
path(8, file=feff0008.dat, e0 = e, s02 = s, sigma2 = s1)
path(9, file=feff0015.dat, e0 = e, s02 = s, sigma2 = s1)
# define FFT Paramters
 set (kmin = 1.0, kmax =10.0)
 set (kweight=3,dk = 1, kwindow='hanning')
 set (rmin = 0, rmax = 6)
 # Create Initial Guess
 ff2chi(1-100,group = init)
 # Do the fit
 feffit(1-100,chi = newdata.chi, group=fit )
 fftf(newdata.chi)
 # Write results
 show @paths
 show @variables
 fit3.chi = fit.chi*fit.k**3
 init3.chi = init.chi*init.k**3
 write_data(file=my_chi.chi3,fit.k,fit3.chi)
 write_data(file=fit_dw_init.DAT,init.k,init3.chi)
 write_data(file=my_FT.out,fit.r,fit.chir_mag)
 write_data(file=exp_r.out,newdata.r,newdata.chir_mag)
 exit
